# Ignore build output directories
build/
*.o
*.a
*.out
*.elf
*.iso
*.bin
*.img

# Ignore temporary files
*.tmp
*.swp
*.swo
*.bak
*~

# Ignore editor and IDE files
.vscode/
.idea/
*.code-workspace
*.sublime-workspace
*.sublime-project

# Ignore log files
*.log

# Ignore compiled libraries
libs/libc/*.o
libs/libother/*.o

# Ignore kernel object files
kernel/arch/*/*.o
kernel/drivers/*.o
kernel/fs/*.o
kernel/init/*.o
kernel/lib/*.o
kernel/mm/*.o
kernel/net/*.o
kernel/sched/*.o
kernel/*.o

# Ignore build scripts and tools outputs
tools/build_scripts/*.o
tools/debug_tools/*.o
tools/emulators/*.o
tools/testing/*.o
tools/utilities/*.o

# Ignore user-space application binaries
user/bin/*
user/sbin/*
user/usr/apps/*.o
user/usr/lib/*.o

# Ignore generated documentation
docs/doxygen/

# Ignore configuration files generated by build tools
configs/kernel/.config
configs/bootloader/.config

# Ignore QEMU and other emulator related files
*.qcow2
*.vdi
*.vmdk

# Ignore core dumps
core

# Ignore ctags files
tags
TAGS

# Ignore other unwanted files
*.DS_Store
Thumbs.db
